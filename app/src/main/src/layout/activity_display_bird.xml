<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Display_bird"
    android:background="@drawable/img"
    >

    <Spinner
        android:id="@+id/filter"
        android:layout_width="307dp"
        android:layout_height="42dp"
        android:prompt="@string/filter"
        android:spinnerMode="dialog"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.44"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.052"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/bi1"
        android:layout_width="400dp"
        android:layout_height="152dp"
        android:layout_marginStart="11dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.203"
        app:srcCompat="@drawable/bird1"
        tools:ignore="ContentDescription,MissingConstraints" />

    <ImageButton
        android:id="@+id/bi2"
        android:layout_width="400dp"
        android:layout_height="152dp"
        android:layout_marginStart="11dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.487"
        app:srcCompat="@drawable/bird2"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/bi3"
        android:layout_width="400dp"
        android:layout_height="152dp"
        android:layout_marginStart="5dp"
        android:layout_marginEnd="6dp"
        android:backgroundTint="#00FFFFFF"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.749"
        app:srcCompat="@drawable/bird3"
        tools:ignore="MissingConstraints" />
    <ImageView
        android:id="@+id/profile"
        style="@style/ic"
        android:layout_width="39dp"

        android:layout_height="35dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_account" />

    <ImageView
        android:id="@+id/cart_order"
        style="@style/ic"
        android:layout_width="39dp"
        android:layout_height="40dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/add_list"
        app:srcCompat="@drawable/ic_cart" />

    <ImageView
        android:id="@+id/add_list"
        style="@style/ic"
        android:layout_width="40dp"
        android:layout_height="35dp"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/search"
        app:srcCompat="@drawable/ic_add" />

    <ImageView
        android:id="@+id/search"
        style="@style/ic"
        android:layout_width="41dp"
        android:layout_height="39dp"
        android:layout_marginBottom="14dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.754"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_search" />

    <ImageView
        android:id="@+id/home_page"
        style="@style/ic"
        android:layout_width="38dp"
        android:layout_height="33dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/ic_home" />
</androidx.constraintlayout.widget.ConstraintLayout>