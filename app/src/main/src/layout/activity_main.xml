<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/themm"
    tools:context=".MainActivity">

    <TableLayout
        android:id="@+id/tableLayout"
        android:layout_width="408dp"
        android:layout_height="486dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <TableRow
            android:id="@+id/tb1"
            android:layout_width="match_parent"
            android:layout_height="140dp"
            android:background="@color/primary">

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="20dp"
                android:text="قائمة الاعلانات"
                android:textSize="34sp"
                tools:ignore="TextContrastCheck" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="80dp"
            android:padding="15dp">


            <ImageView
                android:id="@+id/bird"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_weight="3"
                android:contentDescription="TODO"
                android:gravity="center"
                app:srcCompat="@drawable/search_cat" />

            <EditText
                android:id="@+id/editTextTextPersonName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center"
                android:inputType="textPersonName"
                android:minHeight="48dp"
                android:text="الأسم"
                android:textSize="14dp" />

            <EditText
                android:id="@+id/editTextTextPersonName2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center"
                android:inputType="textPersonName"
                android:minHeight="48dp"
                android:text="النوع"
                android:textSize="14dp" />

            <ImageButton
                android:id="@+id/imgbtn1"
                style="@style/ic"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@android:drawable/ic_menu_more"
                tools:ignore="SpeakableTextPresentCheck" />

            <ImageButton
                android:id="@+id/imageButton15"
                android:layout_width="30dp"
                android:layout_height="35dp"
                android:layout_marginEnd="16dp"
                android:onClick="showAlertDialog"
                app:srcCompat="@drawable/ic_close" />


        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="15dp">

            <ImageView
                android:id="@+id/cat"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_weight="3"
                android:gravity="center"
                app:srcCompat="@drawable/search_rabbit" />

            <EditText
                android:id="@+id/editTextTextPersonName3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center"
                android:inputType="textPersonName"
                android:minHeight="48dp"
                android:text="الأسم"
                android:textSize="14dp" />

            <EditText
                android:id="@+id/editTextTextPersonName4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center"
                android:inputType="textPersonName"
                android:minHeight="48dp"
                android:text="النوع"
                android:textSize="14dp" />

            <ImageButton
                android:id="@+id/imgbtn2"
                style="@style/ic"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@android:drawable/ic_menu_more"
                tools:ignore="SpeakableTextPresentCheck" />

            <ImageButton
                android:id="@+id/imageButton8"
                android:layout_width="30dp"
                android:layout_height="35dp"
                android:layout_marginEnd="16dp"
                android:onClick="showAlertDialog2"
                app:srcCompat="@drawable/ic_close" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="15dp">

            <ImageView
                android:id="@+id/fish"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_weight="3"
                android:gravity="center"
                app:srcCompat="@drawable/search_fish" />

            <EditText
                android:id="@+id/editTextTextPersonName5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center"
                android:inputType="textPersonName"
                android:minHeight="48dp"
                android:text="الأسم"
                android:textSize="14dp" />

            <EditText
                android:id="@+id/editTextTextPersonName6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center"
                android:inputType="textPersonName"
                android:minHeight="48dp"
                android:text="النوع"
                android:textSize="14dp" />

            <ImageButton
                android:id="@+id/imgbtn300"
                style="@style/ic"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@android:drawable/ic_menu_more"
                tools:ignore="SpeakableTextPresentCheck" />

            <ImageButton
                android:id="@+id/imageButton13"
                android:layout_width="30dp"
                android:layout_height="35dp"
                android:layout_marginEnd="16dp"
                android:onClick="showAlertDialog3"
                app:srcCompat="@drawable/ic_close" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="26dp"
            android:padding="15dp">

            <ImageView
                android:id="@+id/hamster"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_weight="3"
                android:gravity="center"
                app:srcCompat="@drawable/search_bird" />

            <EditText
                android:id="@+id/editTextTextPersonName7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center"
                android:inputType="textPersonName"
                android:minHeight="48dp"
                android:text="الأسم"
                android:textSize="14dp" />

            <EditText
                android:id="@+id/editTextTextPersonName8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center"
                android:inputType="textPersonName"
                android:minHeight="48dp"
                android:text="النوع"
                android:textSize="14dp" />

            <ImageButton
                android:id="@+id/imgbtn4"
                style="@style/ic"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:srcCompat="@android:drawable/ic_menu_more"
                tools:ignore="SpeakableTextPresentCheck" />

            <ImageButton
                android:id="@+id/imageButton14"
                android:layout_width="30dp"
                android:layout_height="35dp"
                android:layout_marginEnd="16dp"
                android:onClick="showAlertDialog4"
                app:srcCompat="@drawable/ic_close" />

        </TableRow>


    </TableLayout>

    <ImageView
        android:id="@+id/profile"
        style="@style/ic"
        android:layout_width="39dp"

        android:layout_height="35dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_account" />

    <ImageView
        android:id="@+id/cart_order"
        style="@style/ic"
        android:layout_width="39dp"
        android:layout_height="40dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/add_list"
        app:srcCompat="@drawable/ic_cart" />

    <ImageView
        android:id="@+id/add_list"
        style="@style/ic"
        android:layout_width="40dp"
        android:layout_height="35dp"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/search"
        app:srcCompat="@drawable/ic_add" />

    <ImageView
        android:id="@+id/search"
        style="@style/ic"
        android:layout_width="41dp"
        android:layout_height="39dp"
        android:layout_marginBottom="14dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.754"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_search" />

    <ImageView
        android:id="@+id/home_page"
        style="@style/ic"
        android:layout_width="38dp"
        android:layout_height="33dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/ic_home" />

    <ImageButton
        android:id="@+id/add_button2"
        style="@style/ic"
        android:layout_width="23dp"
        android:layout_height="63dp"
        android:layout_marginBottom="208dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_add_circle" />

</androidx.constraintlayout.widget.ConstraintLayout>