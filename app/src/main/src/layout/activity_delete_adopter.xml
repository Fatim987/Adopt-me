<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".delete_adopter"
    android:background="@drawable/themm">
    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TableRow
            android:id="@+id/tb1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#FFC107">

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:padding="20dp"
                android:text="قائمة الطلبات:"
                android:textSize="34sp" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="80dp">


        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >



            <ImageView
                android:id="@+id/ffish"
                android:layout_width="100dp"
                android:layout_height="80dp"
                android:layout_weight="1"
                android:gravity="center"
                app:srcCompat="@drawable/cat" />

            <EditText
                android:id="@+id/editTextTextPersonName3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center"
                android:inputType="textPersonName"
                android:minHeight="48sp"
                android:text="Name"
                android:textSize="14sp" />


            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center"

                android:minHeight="48sp"
                android:text="حالة الطلب"
                android:textSize="14sp" />

            <ImageButton
                android:id="@+id/a"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="@style/ic"
                app:srcCompat="@android:drawable/btn_dialog" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >


            <ImageView
                android:id="@+id/rrab"
                android:layout_width="100dp"
                android:layout_height="80dp"
                android:layout_weight="1"
                android:gravity="center"
                app:srcCompat="@drawable/search_fish" />

            <EditText
                android:id="@+id/editTextTextPersonName5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center"
                android:inputType="textPersonName"
                android:minHeight="48sp"
                android:text="Name"
                android:textSize="14sp" />

            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center"

                android:minHeight="48sp"
                android:text="حالة الطلب"
                android:textSize="14sp" />

            <ImageButton
                android:id="@+id/a"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="@style/ic"
                app:srcCompat="@android:drawable/btn_dialog" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="26dp" >


            <ImageView
                android:id="@+id/bbird"
                android:layout_width="100dp"
                android:layout_height="60dp"
                android:layout_weight="1"
                android:gravity="center"
                app:srcCompat="@drawable/bird" />

            <EditText
                android:id="@+id/editTextTextPersonName7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center"
                android:inputType="textPersonName"
                android:minHeight="48sp"
                android:text="Name"
                android:textSize="14sp" />


            <TextView

                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:ems="10"
                android:gravity="center"

                android:minHeight="48dp"
                android:text="حالة الطلب"
                android:textSize="14sp" />

            <ImageButton
                android:id="@+id/a"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                style="@style/ic"
                app:srcCompat="@android:drawable/btn_dialog" />

        </TableRow>


    </TableLayout>

    <ImageView
        android:id="@+id/profile"
        style="@style/ic"
        android:layout_width="39dp"

        android:layout_height="35dp"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_account" />

    <ImageView
        android:id="@+id/cart_order"
        style="@style/ic"
        android:layout_width="39dp"
        android:layout_height="40dp"
        android:layout_marginEnd="56dp"
        android:layout_marginBottom="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/add_list"
        app:srcCompat="@drawable/ic_cart" />

    <ImageView
        android:id="@+id/add_list"
        style="@style/ic"
        android:layout_width="40dp"
        android:layout_height="35dp"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="18dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/search"
        app:srcCompat="@drawable/ic_add" />

    <ImageView
        android:id="@+id/search"
        style="@style/ic"
        android:layout_width="41dp"
        android:layout_height="39dp"
        android:layout_marginBottom="14dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.754"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@drawable/ic_search" />

    <ImageView
        android:id="@+id/home_page"
        style="@style/ic"
        android:layout_width="38dp"
        android:layout_height="33dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/ic_home" />

</androidx.constraintlayout.widget.ConstraintLayout>